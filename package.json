{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "react boilerplate hmr starter pack",
  "main": "index.js",
  "dependencies": {
    "@kadira/storybook": "~2.35.3",
    "babel-register": "~6.24.0",
    "classnames": "~2.2.5",
    "koa": "~2.2.0",
    "koa-bodyparser": "~3.2.0",
    "koa-compose": "~3.2.1",
    "koa-convert": "~1.2.0",
    "koa-cors": "0.0.16",
    "koa-helmet": "~2.0.0",
    "koa-logger": "~2.0.0",
    "koa-request": "~1.0.0",
    "koa-rewrite": "~2.1.0",
    "koa-router": "~7.0.1",
    "koa-send": "~3.3.0",
    "koa-static": "~2.0.0",
    "lodash": "~4.17.2",
    "normalize.css": "~5.0.0",
    "react": "~15.4.0",
    "react-dom": "~15.4.0",
    "react-hot-loader": "~3.0.0-beta.6",
    "react-if": "~2.1.0",
    "react-redux": "~5.0.0",
    "react-router": "~3.0.3",
    "redux": "~3.6.0",
    "reselect": "~3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "~6.7.7",
    "babel-cli": "~6.24.0",
    "babel-core": "~6.24.0",
    "babel-eslint": "~7.2.1",
    "babel-jest": "~19.0.0",
    "babel-loader": "~7.0.0-beta.1",
    "babel-plugin-transform-class-properties": "~6.23.0",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-plugin-transform-flow-strip-types": "~6.22.0",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-preset-es2015": "~6.24.0",
    "babel-preset-react": "~6.23.0",
    "babel-preset-stage-0": "~6.22.0",
    "clean-webpack-plugin": "~0.1.14",
    "concurrently": "~3.4.0",
    "cross-env": "~3.2.4",
    "css-loader": "~0.27.3",
    "css-module-flow": "~1.0.0",
    "enzyme": "~2.8.2",
    "eslint": "~3.18.0",
    "eslint-config-airbnb": "~13.0.0",
    "eslint-import-resolver-webpack": "~0.8.1",
    "eslint-loader": "~1.6.1",
    "eslint-plugin-flowtype": "~2.30.3",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "~6.8.0",
    "extract-text-webpack-plugin": "~2.1.0",
    "file-loader": "~0.10.1",
    "flow-babel-webpack-plugin": "~1.0.1",
    "html-webpack-plugin": "~2.28.0",
    "image-webpack-loader": "~3.1.0",
    "jest-cli": "~19.0.2",
    "node-sass": "~4.5.2",
    "nodemon": "~1.11.0",
    "postcss-loader": "~1.3.3",
    "pre-commit": "~1.2.2",
    "raw-loader": "~0.5.1",
    "react-addons-test-utils": "~15.5.1",
    "resolve-url-loader": "~2.0.2",
    "sass-loader": "~6.0.3",
    "style-loader": "~0.16.1",
    "stylefmt": "~5.3.1",
    "stylelint": "~7.9.0",
    "stylelint-config-standard": "~16.0.0",
    "stylelint-order": "~0.4.3",
    "url-loader": "~0.5.7",
    "webpack": "~2.3.2",
    "webpack-dev-server": "~2.4.2"
  },
  "scripts": {
    "eslint": "eslint \"app/**/*.js\" --cache --cache-location \"./.cache\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "cross-env NODE_ENV=production webpack",
    "deploy": "npm run build && npm run server",
    "deploy:dev": "npm run build && npm run server:dev",
    "server": "cross-env NODE_ENV=production node server/index.js",
    "server:dev": "cross-env NODE_ENV=development nodemon server/index.js",
    "start": "concurrently --kill-others \"npm run dev\" \"npm run server:dev\"",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open",
    "storybook": "cross-env NODE_ENV=development start-storybook -p 9001 -c .storybook",
    "storybook:build": "cross-env NODE_ENV=production build-storybook -c .storybook -o .storybook/build",
    "autofix_scss": "stylefmt --recursive 'app/**/*.scss'",
    "stylelint": "stylelint \"app/**/*.scss\""
  },
  "pre-commit": {
    "silent": true,
    "run": [
      "eslint",
      "stylelint",
      "test"
    ]
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  },
  "engines": {
    "node": "~6.9.0",
    "npm": "~3.10.0"
  },
  "jest": {
    "globals": {
      "DEBUG": true
    },
    "roots": [
      "<rootDir>/app"
    ],
    "modulePaths": [
      "<rootDir>/app/"
    ],
    "setupFiles": [
      "<rootDir>/setup/jest-setup.js"
    ],
    "cacheDirectory": "<rootDir>/.cache",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "\\.scss$": "<rootDir>/.mocks/style.js",
      "\\.path$": "<rootDir>/.mocks/path.js"
    }
  },
  "author": "one-more",
  "license": "MIT"
}
