{
    "presets": ["es2015", "stage-0", "react"],
    "plugins": [
        "transform-decorators-legacy",
        "transform-class-properties",
        [
            "transform-runtime", {
            "helpers": false,
            "polyfill": false,
            "regenerator": true
        }
        ],
        "transform-flow-strip-types"
    ]
}
